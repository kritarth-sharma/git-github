pipeline {
   agent any

   stages {
      stage('BUILD') {
         steps {
            echo 'Hello World runnig build phase...'
         }
      }
      stage ('deploy') {
          steps {
            echo 'hey,hi,hello,bye'
            sh '''#!/bin/bash
                echo "who i'm $SHELL"
            '''      
          }
      }
   }
}
